testfile=1000Mrandom
sizes=(10000000 20000000 40000000 80000000 160000000 240000000 320000000 480000000 640000000 800000000 1000000000)

for size in "${sizes[@]}"
do
	echo "building suffix tree for file of size $size"
	cut -c1-$size ../julians-branch/testfiles/$testfile > tmp
	./mergeSuffixTreeTest f tmp
	rm tmp
	echo -e "\n"
done